                    .tabla_totales tr th,.tabla_totales tr td{
                        width: 15%;
                        padding-top: 5px;
                        padding-left: 2px;
                    }

                    .tabla_totales tr td.celda_larga,.tabla_totales tr th.celda_larga{
                        border: none;
                        width: 40%;
                        padding-top: 5px;
                    }

                    .tabla_totales tr.ultima_linea > td{
                        border-right: none;
                        border-left: none;
                        border-bottom: none;
                        border-top: 1.5px solid black;
                    }

                    .etiqueta{
                        position: absolute;
                        top:-10px;
                        left: 50px;
                        background-color: white;
                    }

                    .bordeado{
                        border: 1px solid #cacaca;
