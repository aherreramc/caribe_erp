<openerp>
    <data>
        <record id="erp.nomencladores_embalaje_action" model="ir.actions.act_window">
            <field name="res_model">erp.nomencladores.embalaje</field>
            <field name="name">Embalajes</field>
        </record>

        <record model="ir.ui.view" id="erp.nomencladores_embalaje_lista">
            <field name="name">Embalajes</field>
            <field name="model">erp.nomencladores.embalaje</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" string="Embalaje"/>
                </tree>
            </field>
        </record>

        <record id="embalaje_form" model="ir.ui.view">
            <field name="name">Embalajes</field>
            <field name="priority" eval="0"/>
            <field name="model">erp.nomencladores.embalaje</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group col="2">
                            <field name="name" string="Embalaje"/>
                            <field name="compannia" string="Compañía"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</openerp>